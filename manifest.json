{
    "manifest_version": 3,
    "name": "NotePin",
    "version": "1.0",
    "description": "A simple note-taking Chrome extension.",
    "icons": {
        "16": "icons/Plus.png",
        "48": "icons/Plus1.png"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/Plus.png"
    }
}
